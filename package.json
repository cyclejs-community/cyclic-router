{
  "name": "cyclic-router",
  "version": "1.0.0",
  "description": "A router designed for Cycle.js",
  "main": "lib/index.js",
  "scripts": {
    "docs": "rm -rf docs && mkdir docs && documentation build -f 'html' -g -o docs/ src/*.js",
    "lint": "eslint src/",
    "mocha": "mocha -r babel-register test/index.js",
